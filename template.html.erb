<html>
  <body>
    <table>
      <tr>
        <th>Subject</th>
        <th>Units</th>
        <th>Level</th>
        <th>Title</th>
        <th>Meets</th>
        <th>Instructors</th>
      </th>
      <% @courses.each_with_index do |course, index| %>
      <tr>
        <td><%= get_num(index) %></td>
        <td><%= get_units(index) %></td>
        <td><%= get_level(index) %></td>
        <td><%= get_title(index) %></td>
        <td>
          <% if is_lecture?(index) %>
            Lecture: <%= get_lecture_times(index) %> (<%= get_lecture_room(index) %>) <br />
          <% end %>
        </td>
        <td><%= get_instructors(index) %></td>
      </tr>
      <% end %>
    </table>
  </body>
</html>
